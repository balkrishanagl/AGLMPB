/* range slider code*/
.slider {
    width: 100%;
    margin-top: 25px;
}

input[type="range"]
{
    -webkit-appearance: none !important;
    width: 100%;
    height: 4px;
    background-color: #0f4577;
    border: 1px solid #0f4577;
    border-radius: 10px;
    margin: auto;
    transition: all 0.3s ease;
}
input[type="range"]:hover
{
    background-color: #3680c5;
}

input[type="range"]:focus
{
    outline: none;
}

input[type="range"]::-webkit-slider-thumb
{
    -webkit-appearance: none !important;
    width: 30px;
    height: 30px;
    background-color: #0f4577;
    border-radius: 40px;
    box-shadow: 0px 0px 3px #3c6d59;
    transition: all 0.5s ease;
}

input[type="range"]::-webkit-slider-thumb:hover
{
    background-color: #0f4577;
}

input[type="range"]::-webkit-slider-thumb:focus
{
    outline: none;
}

input[type="range"]::-webkit-slider-thumb:active
{
    box-shadow: 0px 0px 1px #0f4577;
}

input[type=range]::-moz-range-track
{
    background: #0f4577;
    border: none;
    border-radius: 3px;
}

input[type=range]::-moz-range-thumb
{
    border: none;
    height: 30px;
    width: 30px;
    border-radius: 50%;
    background: #0f4577;;
}

