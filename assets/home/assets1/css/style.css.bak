.details p
{
    font-size: 12px;
    font-family: "Lato";
    color: #212121;
    line-height: 25px !important;
}

.center
{
    text-align: center;
}

.pagination
{
    font-size: 20px;
    font-family: "Lato";
    margin: 30px 0 50px 0;
}

.pagination a
{
    color: #666666;
    float: left;
    padding: 8px 16px;
    text-decoration: none;
}

.pagination a.active
{
    background-color: #0f4577;
    color: white;
    border-radius: 50px;
    text-decoration: none;
}

.pagination a:hover:not(.active)
{
    background-color: #ddd;
    border-radius: 50px;
    text-decoration: none;
}

.carousel_img
{
   padding-right: 15px;padding-left: 15px;
}

.footer
{
    background-color: black;
    color: white;
    padding: 25px;
    padding-bottom: 10px;
}
.footer a
{
    color: #cdcdcd;
    font-size: 14px;
    font-family: "Lato";
    line-height: 2.5;
}
.footer h3
{
    font-size: 18px;
    font-family: "Lato";
    line-height: 1;
	color: #ADB0B6;
    font-weight: 700;
}

.footer a:hover
{
    color: white;
    text-decoration: none;
}
.footer-links-big {padding-left:1px;}
.footer-links-big li {list-style-type:none;}
.fea-div {min-height:50px;}
.fea-div img {width:20%;height:auto;}
.pro-sec {margin-top:2% !important;background-color: #fff; padding: 0px;width: 48%;margin-right: 2%;}
.pro-sec .col-md-4 {padding: 0;}
.view-sect a {color:#fff;}
.view-sect {text-align: center;padding: 2%;background-color: #269C57;border-bottom-radius: 5px;}
