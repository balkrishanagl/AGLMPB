.text h4
{
    font-size: 16px;
    font-family: "Lato";
    color: #212121;
    line-height: 1.2;
}

.text p
{
    font-size: 12px;
    font-family: "Lato";
    color: #333333;
    line-height: 1.2;
}

.last_p
{
    margin-top: 10px;
}

.number
