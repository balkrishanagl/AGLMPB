}

.box {
  border-style: solid;
  border-width: 1px;
  /*opacity: 0.2;*/
  box-shadow: 0px 2px 5px 0px grey;
  margin-top: 40px;
  border-radius: 0px;
}
.box .panel-footer{
    text-align: center;
    font-size: 18px;
}
.post i{
    color: #0F4577;
}
.card
{
    padding: 0px;
}

.card_pad
{
    margin-right: 20px;
}

.buy h3, .quick_search h3
{
    font-size: 26px;
    font-family: "Lato";
    color: #0F4577;
    line-height: 1.2;
    margin-top: 30px;
}

.buy p
{
    font-size: 12px;
    font-family: "Lato";
    color: #212121;
    line-height: 1.2;
}

