    font-size: 22px;
    font-family: "Lato";
    color: #333333;
    line-height: 1.2;
}

.form_box
{
    box-shadow: 0px 5px 5px 0px rgba(17, 17, 17, 0.15);
    margin-top: 29px;
    padding-bottom: 10px;
}

#inner_form
{
    margin-right: 10%;
    margin-left: 10%;
    padding-top: 10%;
}

.text_area
{
    border-radius: 3px;
    background-color: #F4F4F4;
    font-size: 18px;
    font-family: "Lato";
    color: #666666;
    line-height: 1.2;
    padding: 6px 12px;
	height: 37px;margin-bottom: 10px;
}

.form_btn
{
    font-family: "Lato";
    color: white;
    line-height: 1.2;
    background-color: #269c57;
    margin-top: 40px;
	font-size: 16px;padding: 5px 35px;
}

.lg_btn {
    font-family: "Lato";
    color: white;
    background-color: #0f4577;
    line-height: 1.2;
    border-radius: 2px;
    margin-top: 15px;
	width: 98%;margin-left:0px;font-size: 16px;padding: 9px 0;
}
.lg_btn:hover {color:#fff;}
.content
{
    padding-top: 30px;
    margin-top: 20px;
    margin-bottom: 30px;
}

.icons a
{
    font-size: 20px;
    font-family: "Lato";
    color: #212121;
    line-height: 1.2;
    text-align: center;
    text-decoration: none;
}

.icons a:hover
{
    text-decoration: none;
    color: #212121;
}

.img_icon
{
    height: 88px;
    margin-bottom: 10px;
}

.details
{
    margin: 15px;
    margin-top: 50px;
}

.details h3
{
    font-size: 16px;font-weight: 600;
    font-family: "Lato";
    color: #212121;
    text-decoration: underline;
    line-height: 1.2;
    margin-top: 0;
}
.sub_p
{
    font-size: 14px;
    font-family: "Lato";
    color: #666666;
    line-height: 1.2;
}

.details hr
{
    margin-top: 0;
    border-top: 1px solid #bababa;
}

