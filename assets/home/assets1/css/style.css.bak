.img_cover
{
    max-height: 70vh;
    width: 100%;
}
#location{
    float: left;
    font-size: 19px;
}
#searchForm h3
{
    color: white;
    text-align: center;
    font-family: "Lato";
    line-height: 1.2;
    margin-bottom: 30px;
}

#searchsubmit
{
    font-family: "Lato";
    line-height: 46px;
    letter-spacing: 1px;
    font-size: 18px;
    padding: 0 20px;
    border: 0;
    background: #0f4577;
    color: white;
    border-radius: 5px;
    margin-left: -4px;
}

#searchForm input#s
{
    font-family: "Lato";
    font-size: 15px;
    color: #1f1f1f;
    background-color: #f5f5f5;
    line-height: 46px;
    padding: 0 20px;
    cursor: text;
    border: 0;
    width: 100%;
    margin-bottom: 0px;
    transition: background-color 0.5s;
    border-radius: 5px;
    margin-left: -4px;
}

